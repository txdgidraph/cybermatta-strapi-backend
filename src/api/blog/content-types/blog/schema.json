{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "blogBody": {
      "type": "richtext"
    },
    "FeaturedImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Category": {
      "type": "enumeration",
      "enum": [
        "Technology",
        "News",
        "Featured",
        "HowTo",
        "Gadgets_devices"
      ]
    },
    "PostType": {
      "type": "enumeration",
      "enum": [
        "FeaturedPost",
        "TrendingNews"
      ]
    },
    "Slug": {
      "type": "string"
    },
    "blog_author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::blog-author.blog-author",
      "inversedBy": "blogs"
    }
  }
}
