{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "blogBody": {
      "type": "richtext"
    },
    "FeaturedImage": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "Category": {
      "type": "enumeration",
      "enum": [
        "Technology",
        "News",
        "Featured",
        "HowTo",
        "Gadgets_devices"
      ]
    },
    "PostType": {
      "type": "enumeration",
      "enum": [
        "FeaturedPost",
        "TrendingNews"
      ]
    }
  }
}
